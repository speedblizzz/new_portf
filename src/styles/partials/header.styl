.header
  position: relative;
  padding: 25px 0 45px 0;

@media (min-width: 768px)
  .header
    padding: 45px 0;

@media (min-width: 1600px)
  .header {
    padding: 55px 0 55px 0;
  }


@media (max-width: 1199px)
  .header
    z-index: 100;

.site-header-main
  margin-left: auto;
  margin-right: auto;
  width: 90%;





@media (min-width: 768px)
  .site-header-main
    width: 90%;
@media (min-width: 1000px)
  .site-header-main
    width: 930px;

@media (min-width: 1200px)
  .site-header-main
    width: 1150px;

@media (min-width: 1400px)
  .site-header-main {
    width: 1200px;
  }

.logo-menu-wrapper
  display: flex
  flex-flow row nowrap
  justify-content: space-between
  align-items center
  width: 100%;
.logo
  position relative
  display: flex;
  flex-flow: row nowrap
  justify-content: space-between
  align-items center
  height: 70px
  perspective: 800px;
  overflow hidden

.logo a
  display: block
  height: 100%
  color: #757575;
  text-decoration: none;

.logo img
  height: 100%;
  display: block;

.logo img.site-logo2x
  display: none;

.logo_part_3
  animation-name slideInLeft,hideAfterSlide
  animation-delay 0s,1s
  animation-duration: 0.5s,0.4s;
  animation-fill-mode: both;
  animation-timing-function ease,ease
  transition all .3s



.logo_part_1
  animation-name slideInLeft,hideAfterSlide
  animation-delay 0s,1.4s
  animation-duration: .9s,0.4s;
  animation-fill-mode: both;
  animation-timing-function ease,ease




.logo_part_2

  animation-name slideInLeft,hideAfterSlide
  animation-delay 0s,1.2s
  animation-duration: .7s,0.4s;
  animation-fill-mode: both;
  animation-timing-function ease,ease



.logo_part_4
  animation-name slideInLeft,hideRotateAfterSlide
  animation-delay 0s,1.7s
  animation-duration: 1.1s,0.7s;
  animation-fill-mode: both;
  animation-timing-function ease,ease

.site-logo
  position: absolute
  left 12px
  animation showAfterSlide
  animation-duration: 1s;
  animation-delay 2s
  animation-fill-mode: both;
  transition: transform 1s;

@keyframes showAfterSlide
  0% {

    visibility: hidden;
  }
  100% {
    visibility: visible;
  }
@keyframes slideInLeft
  0% {
    transform: translate3d(-3000%,0,0);
    visibility: visible;
  }
  100% {
      transform: translateZ(0);
  }

@keyframes hideAfterSlide
  0% {

    opacity: 1;
    visibility visible
  }

  100% {
    transform: rotateY( 180deg );
    opacity: 0;
    visibility: hidden;
  }
@keyframes hideRotateAfterSlide
  0% {

    opacity: 1;
    visibility visible

  }

  5% {

  }

  100% {
    transform: rotateY( 180deg );
    opacity: 0;
    visibility hidden
  }

@keyframes slideInLeftFade
  0% {
    transform: translate3d(-3000%,0,0);
    visibility: visible;
  }
  100% {
    transform: translateZ(0);

  }



.site-header-menu
  display: table-cell;
  vertical-align: middle;

@media (max-width: 767px)
  .site-header-menu
    left: -60%;
    width: 60%;


@media (max-width: 1199px)
  .site-header-menu
    position: fixed
    left: -44%
    top: 0
    background-color: #fff
    bottom: 0
    width: 44%
    display: block
    overflow-y: auto
    z-index: 101
    transition: transform 0.4s cubic-bezier(0.64, 0.01, 0.28, 1)

.logo-menu-wrapper .mobile
  display: block;

.logo-menu-wrapper .mobile
  width: 24px;
  height: 20px;
  display: none;
  cursor: pointer;
  position: relative;

@media (max-width: 1199px)
  .logo-menu-wrapper .mobile
    display: block;
    padding-top: 9px

.logo-menu-wrapper .mobile span
  width: 12px;
  height: 2px;
  display: block;
  position: absolute;
  background-color: #222;
  vertical-align: middle;
  -webkit-transition: background-color 0.15s linear;
  -moz-transition: background-color 0.15s linear;
  transition: background-color 0.15s linear;
  &:after,&:before
    content: '';
    width: 12px;
    height: 2px;
    display: block;
    position: absolute;
    background-color: #222;
    -webkit-transition: background-color 0.15s linear, -webkit-transform 0.25s ease;
    -moz-transition: background-color 0.15s linear, -moz-transform 0.25s ease;
    transition: background-color 0.15s linear, transform 0.25s ease;


.logo-menu-wrapper .mobile span.ham-right
  left: 12px;

.logo-menu-wrapper .mobile span:before
  top: -8px;


.logo-menu-wrapper .mobile span:after
  top: 8px;

@media (max-width: 1199px)
  body.menu-is-opened .site-header-menu
    transform: translateX(100%);

.menu-is-opened .logo-menu-wrapper .mobile span {
  background-color: transparent;
}


.menu-is-opened .logo-menu-wrapper .mobile span:before, .menu-is-opened .logo-menu-wrapper .mobile span:after
  background-color: #fff;


:not(.menu-sb).menu-is-opened .logo-menu-wrapper .mobile span.ham-left:before
  transform: rotateZ(45deg) scaleX(1.2) translate(2px, 2px);

:not(.menu-sb).menu-is-opened .logo-menu-wrapper .mobile span.ham-left:after
  transform: rotateZ(-45deg) scaleX(1.2) translate(2px, -2px);


:not(.menu-sb).menu-is-opened .logo-menu-wrapper .mobile span.ham-right:before
  transform: rotateZ(-45deg) scaleX(1.2) translate(-3px, 1px);

:not(.menu-sb).menu-is-opened .logo-menu-wrapper .mobile span.ham-right:after
  transform: rotateZ(45deg) scaleX(1.2) translate(-3px, -1px);



@media (max-width: 767px)
  .site-header-menu {
    left: -60%;
    width: 60%;
  }

@media (max-width: 1199px)
  .site-header-main nav
    position: relative;
    margin: 20px 0;
    font-size: inherit;
    line-height: inherit;
    padding-left: 25px;

.site-header-menu ul.primary-menu
  position: relative;
  list-style: none;
  margin: 0;
  padding: 0;
  text-align: right;

@media (max-width: 1199px)
  .site-header-menu ul.primary-menu
    text-align: left;

.site-header-menu ul.primary-menu > li
    display: inline-block;
    position: relative;
    font-size: 15px;

a
  display: block;
  color: #757575;
  text-decoration: none;

@media (max-width: 1199px)
  .site-header-menu ul.primary-menu > li
    display: block;
    font-size: 20px;

.site-header-menu ul.primary-menu a
  color: #222;
  text-decoration: none;
  padding: 20px 0;
  line-height: 0

@media (min-width: 1600px)
  .site-header-menu ul.primary-menu > li {
    font-size: 16px;
  }



.site-header-menu ul.primary-menu > li + li
  margin-left: 50px;

@media (max-width: 1199px)
  .site-header-menu ul.primary-menu > li + li
    margin-left: 0;


body:not(.menu-sb) .site-header-menu ul.primary-menu .current-menu-item > a:after,
body:not(.menu-sb) .site-header-menu ul.primary-menu .current-menu-parent > a:after
  right 0








@media (max-width: 1199px)
  .site-header-menu ul.primary-menu a
    padding: 5px;
    display: inline-block;
    &:hover
      color: mainColor



@media (min-width: 1200px)
  .site-header-menu ul.primary-menu > li > a:after {
    content: '';
    position: absolute;
    top: -6px;
    border-bottom: 2px solid;
    border-color: mainColor;
    left: 0;
    right: 100%;
    transition: right 0.3s cubic-bezier(0.4, 0, 0.21, 1.1)

    /*transform: scale(0);*/
  }
  .site-header-menu ul.primary-menu  > li:hover > a:after
    right 0
    /*transform: scale(1);*/
    /*color: #055*/




@media (max-width: 1199px)
  body:not(.menu-sb) .site-header-menu ul.primary-menu .current-menu-item > a,
  body:not(.menu-sb) .site-header-menu ul.primary-menu .current-menu-parent > a
    color: #0097A7;

.site-header-menu ul.primary-menu ul.sub-menu {
  position: absolute;
  text-align: left;
  padding: 15px 0;
  min-width: 160px;
  line-height: 1.8em;
  background-color: #444;
  z-index: 50;
  visibility: hidden;
  opacity: 0;
}

@media (max-width: 1199px)
  .site-header-menu ul.primary-menu ul.sub-menu {
    visibility: visible;
    position: relative;
    opacity: 1;
    background-color: transparent;
    padding: 0 0 0 20px;
    display: none;
  }

@media (min-width: 1200px)
  .site-header-menu ul.primary-menu ul.sub-menu {

    transition: visibility 0s linear 0.1s, opacity 0.1s ease;
  }



